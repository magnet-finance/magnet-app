{
    "compilerOptions": {
      "module": "commonjs",
      "target": "esnext",
      "jsx": "preserve",
      "lib": ["dom", "esnext"],
      "strict": true,
      "noEmit": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
    },
    "include": ["src/**/*.ts*"]
  }
